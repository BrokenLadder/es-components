{
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-export-default-from"
  ],
  "env": {
    "development": {
      "presets": [ ["wtw-im", {
        "extractFormatMessage": false,
        "transformFormatMessage": false
      }] ]
    },
    "es6": {
      "presets": [ ["wtw-im", {
        "env": {
          "modules": false
        },
        "extractFormatMessage": false,
        "transformFormatMessage": false
      }] ]
    },
    "production": {
      "presets": [ ["wtw-im", {
        "extractFormatMessage": false,
        "transformFormatMessage": false
      }] ]
    },
    "test": {
      "presets": [ ["wtw-im", {
        "extractFormatMessage": false,
        "transformFormatMessage": false
      }] ]
    },
    "styleguidist": {
      "presets": [ ["wtw-im", {
        "extractFormatMessage": false,
        "transformFormatMessage": false
      }] ]
    }
  }
}
